{
    "main": {
        "projectName": {
            "elementType": "inputText",
            "label": "project name",
            "placeholder": "name",
            "value": "string"
        },
        "projectStatus": {
            "elementType": "select",
            "label": "project status",
            "value": "array",
            "options": [
                "new",
                "planing",
                "production",
                "finished",
                "updating",
                "bug fixing",
                "canceled"
            ]
        },
        "projectStartDate": {
            "elementType": "inputDate",
            "label": "start date",
            "value": "string"
        },
        "projectDeadLine": {
            "elementType": "inputDate",
            "label": "deadline",
            "value": "string"
        },
        "repositoryLink": {
            "elementType": "inputText",
            "label": "repository",
            "placeholder": "web address",
            "value": "string"
        },
        "onlineAddrss": {
            "elementType": "inputText",
            "label": "online ",
            "placeholder": "web addres",
            "value": "string"
        },
        "projectAssumptions": {
            "elementType": "inputText",
            "label": "assumptions",
            "placeholder": "assumption",
            "numberOfInputs": 1,
            "value": "array"
        },
        "projectNotes": {
            "elementType": "inputText",
            "label": "project notes",
            "placeholder": "note",
            "numberOfInputs": 1,
            "value": "array"
        },
        "toDo": {
            "elementType": "inputText",
            "label": "project todos name",
            "placeholder": "todo name",
            "numberOfInputs": 1,
            "showDetails": false,
            "value": "object"
        }
    },
    "secondary": [
        {
            "parent": "toDo",
            "details": {
                "isActive": {
                    "value": "boolean",
                    "defaultValue": true,
                    "startValue": true
                },
                "status": {
                    "elementType": "select",
                    "label": "todo status",
                    "value": "array",
                    "options": [
                        "new",
                        "in process",
                        "resolved",
                        "failed",
                        "rejected"
                    ]
                },
                "priority": {
                    "elementType": "select",
                    "label": "priority",
                    "value": "array",
                    "options": [
                        "low",
                        "medium",
                        "high"
                    ]
                },
                "description": {
                    "elementType": "inputText",
                    "label": "to description",
                    "placeholder": "description",
                    "value": "string"
                },
                "note": {
                    "elementType": "inputText",
                    "label": "todo notes",
                    "placeholder": "todo note",
                    "numberOfInputs": 1,
                    "value": "array"
                }
            }
        }
    ],
    "controls": {
        "temporaryValue": null,
        "showToDoDetails": null
    }
}