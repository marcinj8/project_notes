    "secondary": {
        "todoDetails": {
            "isActive": true,
            "todoStatus": {
                "elementType": "select",
                "label": "todo status",
                "options": [
                    "new",
                    "in process",
                    "resolved",
                    "failed",
                    "rejected"
                ]
            },
            "priority":{
                "elementType": "select",
                "label": "priority",
                "options": [
                    "low",
                    "medium",
                    "high"
                ]
            },
            "description":{
                "elementType": "inputText",
                "label": "project description",
                "placeholder": "description",
                "value": "string"
            } ,
            "toDoNote": {
                "elementType": "inputText",
                "label": "todo notes",
                "placeholder": "todo note",
                "numberOfInputs": 1,
                "value": "array"
            }
        }
    }
